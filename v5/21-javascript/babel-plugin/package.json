  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.0",
    "@babel/plugin-transform-arrow-functions": "7.13.0",
    "@babel/plugin-transform-block-scoping": "7.14.1"
  }
}
